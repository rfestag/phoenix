  "scripts": {
    "watch-dark": "node-sass-chokidar --include-path=./node_modules --include-path=src/stylesheets src/stylesheets/dark.scss -o public/ --watch --recursive",
    "watch-light": "node-sass-chokidar --include-path=./node_modules --include-path=src/stylesheets src/stylesheets/light.scss -o public/ --watch --recursive",
    "watch-css": "npm run build-css && npm-run-all -p watch-dark watch-light",
    "start-styleguide": "npx styleguidist server",
    "start-docs": "esdoc && static docs",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build-docs": "esdoc",
    "build-light": "node-sass-chokidar --include-path=./node_modules --include-path=src/stylesheets src/stylesheets/light.scss -o public/",
    "build-dark": "node-sass-chokidar --include-path=./node_modules --include-path=src/stylesheets src/stylesheets/dark.scss -o public/",
    "build-css": "npm-run-all -p build-dark build-light",
    "build-js": "react-app-rewired build",
    "build": "npm-run-all -p build-css build-js build-docs",
    "test": "react-app-rewired test --env=jsdom",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "cross-env CI=true react-app-rewired test --env=jsdom --findRelatedTests",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.shared\\.js": "<rootDir>/__mocks__/sharedMock.js"
    }
  },
  "browserslist": {
    "development": [
      "Firefox >= 38"
    ],
    "production": [
      "Firefox >= 38"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.1.6",
    "esdoc": "^1.1.0",
    "esdoc-coverage-plugin": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "prettier": "1.12.1",
    "stylelint": "^9.9.0",
    "stylelint-scss": "^3.5.0",
    "vue": "2.5.16"
  }
}
